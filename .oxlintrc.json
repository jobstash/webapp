{
  "ignorePatterns": ["node_modules/", "public/"],
  "categories": {
    "correctness": "warn"
  },
  "rules": {
    "eslint/no-unused-vars": "error",
    "jsx-a11y/click-events-have-key-events": "off"
  },
  "plugins": [
    "react",
    "react-perf",
    "nextjs",
    "jsx-a11y",
    "unicorn",
    "import",
    "typescript",
    "oxc"
  ]
}
