{
  "name": "webapp",
  "version": "2.1.3",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "pnpm format && pnpm lint",
    "build": "next build",
    "build:disable-static": "DISABLE_STATIC_GENERATION=true next build",
    "start": "next start",
    "format": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "format:fix": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "lint": "next lint && oxlint --fix --fix-suggestions",
    "test": "echo \"Skipping tests ...\" && exit 0",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "postinstall": "simple-git-hooks",
    "push": "git push origin $(git rev-parse --abbrev-ref HEAD)",
    "validate-version": "node scripts/validate-version.js",
    "bump:patch": "node scripts/bump-version.js patch",
    "bump:minor": "node scripts/bump-version.js minor"
  },
  "dependencies": {
    "@leeoniya/ufuzzy": "^1.0.18",
    "@privy-io/react-auth": "^2.21.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@sentry/nextjs": "^9.6.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.71.1",
    "@tanstack/react-virtual": "^3.13.6",
    "@xstate/react": "^6.0.0",
    "@xstate/store": "^3.8.5",
    "ahooks": "^3.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.1",
    "iron-session": "^8.0.4",
    "ky": "^1.7.4",
    "lucide-react": "^0.483.0",
    "next": "15.4.6",
    "next-themes": "^0.4.6",
    "nuqs": "^2.4.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-intersection-observer": "^9.14.1",
    "server-only": "^0.0.1",
    "slugify": "^1.6.6",
    "storybook-addon-module-mock": "^1.3.4",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "transliteration": "^2.3.5",
    "webapp": "link:",
    "xstate": "^5.20.2",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.3.0",
    "@next/eslint-plugin-next": "15.4.6",
    "@storybook/addon-designs": "^8.2.1",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/blocks": "^8.5.0",
    "@storybook/nextjs": "^8.5.0",
    "@storybook/react": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@tailwindcss/postcss": "^4.0.14",
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@tanstack/react-query-devtools": "^5.71.1",
    "@types/node": "^20",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.11.2",
    "lint-staged": "^15.3.0",
    "msw": "^2.7.0",
    "msw-storybook-addon": "^2.0.4",
    "oxlint": "^1.11.2",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "semver": "^7.7.2",
    "simple-git-hooks": "^2.11.1",
    "storybook": "^8.5.0",
    "tailwindcss": "^4.0.14",
    "typescript": "^5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm dlx commitlint --edit $1",
    "pre-commit": "pnpm lint-staged",
    "pre-push": "pnpm lint && pnpm validate-version && pnpm build:disable-static && pnpm test"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,astro,svelte}": "oxlint --fix --fix-suggestions"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.10",
      "@types/react-dom": "19.1.7"
    },
    "onlyBuiltDependencies": [
      "@reown/appkit",
      "@sentry/cli",
      "bufferutil",
      "core-js-pure",
      "esbuild",
      "msw",
      "secp256k1",
      "sharp",
      "simple-git-hooks",
      "utf-8-validate"
    ]
  }
}
